{
  "manifest_version": 2,

  "name": "PharahWatch",
  "description": "This extension shows OW stats",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://playoverwatch.com/",
    "tabs", "http://*/*", "https://*/*"

  ]
}
